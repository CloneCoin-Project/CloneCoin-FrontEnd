{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@apis": [
        "./src/apis",
        "./src/apis/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@pages/*": [
        "./src/pages/*",
        "./src/pages"
      ],
      "@hooks/*": [
        "./src/hooks/*",
        "./src/hooks"
      ],
      "@styles/*": [
        "./src/styles/*",
        "./src/styles"
      ],
      "@store": [
        "./src/store",
        "./src/store/*"
      ]
    }
  }
}
